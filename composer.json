{
    "name": "twogether",
    "description": "An microservice to handle txt to CSV",
    "keywords": ["twogether","txt-to-csv"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.4",
        "predis/predis": "^1.1",
        "rapidwebltd/php-uk-bank-holidays": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
